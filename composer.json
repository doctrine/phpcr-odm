{
    "name": "doctrine/phpcr-odm",
    "type": "library",
    "description": "Object-Document-Mapper for PHPCR",
    "keywords": ["odm", "phpcr", "database"],
    "homepage": "http://www.doctrine-project.org",
    "license": "MIT",
    "authors":[
        {"name":"Jordi Boggiano", "email":"j.boggiano@seld.be"},
        {"name":"David Buchmann", "email":"david@liip.ch"},
        {"name":"Lukas Kahwe Smith", "email":"smith@pooteeweet.org"}
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "doctrine/common": "~2.4",
        "doctrine/annotations": "~1.2",
        "doctrine/data-fixtures": "~1.0,>=1.0.0",
        "phpcr/phpcr": "~2.1.1",
        "phpcr/phpcr-implementation": "~2.1.0",
        "phpcr/phpcr-utils": "~1.1",
        "doctrine/instantiator": "~1.0.1",
        "symfony/console": "~2.3"
    },
    "require-dev": {
        "aferrandini/urlizer": "1.0.*",
        "symfony/yaml": "~2.0",
        "symfony/phpunit-bridge": "2.7.*@dev",
        "liip/rmt": "~1.1"
    },
    "suggest": {
        "symfony/yaml": "~2.0",
        "jackalope/jackalope-doctrine-dbal": "~1.1",
        "jackalope/jackalope-jackrabbit": "~1.0"
    },
    "conflict": {
        "jackalope/jackalope-doctrine-dbal": "<1.1.5"
    },
    "autoload": {
        "psr-0": { "Doctrine\\ODM\\PHPCR": "lib/" }
    },
    "bin": ["bin/phpcrodm", "bin/phpcrodm.php"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    }
}
